msgid ""
"Examples:\n"
"\tgawk '{ sum += $1 }; END { print sum }' file\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"
msgstr ""
"Contoh:\n"
"\tgawk '{ sum += $1 }; END { print sum }' berkas\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"

