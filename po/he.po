#: array.c:249
#, c-format
msgid "from %s"
msgstr "מ־%s"

#: array.c:355
msgid "attempt to use a scalar value as array"
msgstr "ניסיון שימוש בסקלאר כמערך"

#: array.c:357
#, c-format
msgid "attempt to use scalar parameter `%s' as an array"
msgstr "נסיון שימוש במשתנה סקלרי `%s' כמערך"

#: array.c:360
#, c-format
msgid "attempt to use scalar `%s' as an array"
msgstr "ניסיון שימוש בסקלאר `%s' כמערך"

#: array.c:407 array.c:574 builtin.c:88 builtin.c:1746 builtin.c:1794
#: builtin.c:1807 builtin.c:2323 builtin.c:2350 eval.c:1156 eval.c:1161 eval.c:1555
#, c-format
msgid "attempt to use array `%s' in a scalar context"
msgstr "נסיון שימוש במערך `%s' בהקשר של סקלאר"

#: array.c:581
#, c-format
msgid "delete: index `%.*s' not in array `%s'"
msgstr "delete: המפתח `‎%.*s' לא נמצא במערך `%s'"

#: array.c:595
#, c-format
msgid "attempt to use scalar `%s[\"%.*s\"]' as an array"
msgstr "ניסיון שימוש בסקלאר `‎%s[\"%.*s\"]' כמערך"

#: array.c:808 array.c:858
#, c-format
msgid "%s: first argument is not an array"
msgstr "%s: הארגומנט הראשון אינו מערך"

#: array.c:850
#, c-format
msgid "%s: second argument is not an array"
msgstr "%s: ארגומנט שני אינו מערך"

#: array.c:853 field.c:1006 field.c:1100
#, c-format
msgid "%s: cannot use %s as second argument"
msgstr "%s: אסור להשתמש ב־`%s' כארגומנט שני"

#: array.c:861
#, c-format
msgid "%s: first argument cannot be SYMTAB without a second argument"
msgstr "%s: ארגומנט ראשון לא יכול להיות SYMTAB ללא ארגומנט שני"

#: array.c:863
#, c-format
msgid "%s: first argument cannot be FUNCTAB without a second argument"
msgstr "%s: ארגומנט ראשון לא יכול להיות FUNCTAB ללא ארגומנט שני"

#: array.c:870
msgid ""
"asort/asorti: using the same array as source and destination without a third "
"argument is silly."
msgstr "asort/asorti: שימוש באותו מערך כמקור ויעד ללא ארגומנט שלישי הוא טיפשי."

#: array.c:875
#, c-format
msgid "%s: cannot use a subarray of first argument for second argument"
msgstr "%s: לא יכול להשתמש במערך משנה של ארגומנט ראשון עבור ארגומנט שני"

#: array.c:880
#, c-format
msgid "%s: cannot use a subarray of second argument for first argument"
msgstr "%s: לא יכול להשתמש במערך משנה של ארגומנט שני עבור ארגומנט ראשון"

#: array.c:1410
#, c-format
msgid "`%s' is invalid as a function name"
msgstr "`%s' אינו חוקי כשם פונקציה"

#: array.c:1414
#, c-format
msgid "sort comparison function `%s' is not defined"
msgstr "פונקציית המיון `%s' אינה מוגדרת"

#: awkgram.y:278
#, c-format
msgid "%s blocks must have an action part"
msgstr "בלוק %s חייב לכלול הגדרת פעולה"

#: awkgram.y:281
msgid "each rule must have a pattern or an action part"
msgstr "לכל חוק צריך שיהיה או תבנית או פעולה"

#: awkgram.y:435 awkgram.y:447
msgid "old awk does not support multiple `BEGIN' or `END' rules"
msgstr "awk ישן אינו תומך בכמה חוקי BEGIN או END"

#: awkgram.y:500
#, c-format
msgid "`%s' is a built-in function, it cannot be redefined"
msgstr "`%s' הינה פונקציה מובנית, אין לשנות את הגדרתה"

#: awkgram.y:564
msgid "regexp constant `//' looks like a C++ comment, but is not"
msgstr "הביטוי הרגולרי '//` נראה כמו הערה בשפת C++‎, אך אינו הערה"

#: awkgram.y:568
#, c-format
msgid "regexp constant `/%s/' looks like a C comment, but is not"
msgstr "ביטוי רגולרי '/%s/` נראה כמו הערה בשפת C, אך אינו הערה"

#: awkgram.y:695
#, c-format
msgid "duplicate case values in switch body: %s"
msgstr "כפול ערכי רישיות בגוף המתג: %s"

#: awkgram.y:716
msgid "duplicate `default' detected in switch body"
msgstr "זוהה `default' כפול בגוף המיתוג"

#: awkgram.y:1052 awkgram.y:4484
msgid "`break' is not allowed outside a loop or switch"
msgstr "אין להשתמש ב־`break' מחוץ ללולאה"

#: awkgram.y:1062 awkgram.y:4476
msgid "`continue' is not allowed outside a loop"
msgstr "אין להשתמש ב־`continue' מחוץ ללולאה"

#: awkgram.y:1073
#, c-format
msgid "`next' used in %s action"
msgstr "שימוש ב־`next' בהגדרת פעולה של %s"

#: awkgram.y:1084
#, c-format
msgid "`nextfile' used in %s action"
msgstr "שימוש ב־`nextfile' בהגדרת פעולה של %s"

#: awkgram.y:1112
msgid "`return' used outside function context"
msgstr "שימוש ב-`return` שלא בקונטקסט של פונקציה"

#: awkgram.y:1185
msgid "plain `print' in BEGIN or END rule should probably be `print \"\"'"
msgstr "`print' פשוט בכללי BEGIN או END צריך מן הסתם להיות `‎print \"\"‎'"

#: awkgram.y:1255 awkgram.y:1304
msgid "`delete' is not allowed with SYMTAB"
msgstr "`delete' אסור עם SYMTAB"

#: awkgram.y:1257 awkgram.y:1306
msgid "`delete' is not allowed with FUNCTAB"
msgstr "`delete' אסור עם FUNCTAB"

#: awkgram.y:1291 awkgram.y:1295
msgid "`delete(array)' is a non-portable tawk extension"
msgstr "`delete(array)' הינו הרחבה של tawk לא פורטבלי"

#: awkgram.y:1431
msgid "multistage two-way pipelines don't work"
msgstr "אפיקים דו־כיווניים מרובי שלבים אינם עובדים"

#: awkgram.y:1433
msgid "concatenation as I/O `>' redirection target is ambiguous"
msgstr "השרשור כיעד ניתוב מחדש של I/O `>' אינו חד משמעי"

#: awkgram.y:1645
msgid "regular expression on right of assignment"
msgstr "ביטוי רגולרי בצד ימין של הוראת השמת ערך"

#: awkgram.y:1660 awkgram.y:1673
msgid "regular expression on left of `~' or `!~' operator"
msgstr "ביטוי רגולרי משמאל לאופרטור '~` או '~!`"

#: awkgram.y:1690 awkgram.y:1840
msgid "old awk does not support the keyword `in' except after `for'"
msgstr "awk ישן לא תומך בביטוי `in' למעט אחרי `for'"

#: awkgram.y:1700
msgid "regular expression on right of comparison"
msgstr "ביטוי רגולרי בצד ימין של השוואה"

#: awkgram.y:1819
#, c-format
msgid "non-redirected `getline' invalid inside `%s' rule"
msgstr "`getline' לא מנותב לא חוקי בתוך כלל `%s'"

#: awkgram.y:1822
msgid "non-redirected `getline' undefined inside END action"
msgstr "`getline' ללא הפנייה אינו מוגדר בתוך פעולת END"

#: awkgram.y:1842
msgid "old awk does not support multidimensional arrays"
msgstr "awk ישן אינו תומך במערכים של כמה מימדים"

#: awkgram.y:1945
msgid "call of `length' without parentheses is not portable"
msgstr "קריאה ל־`length' בלי סוגריים אינה ניידת"

#: awkgram.y:2019
msgid "indirect function calls are a gawk extension"
msgstr "קריאות עקיפות לפונקציות הינן הרחבה ל־gawk"

#: awkgram.y:2032
#, c-format
msgid "cannot use special variable `%s' for indirect function call"
msgstr "אסור להשתמש במשתנה מיוחד `%s' בשביל קריאה עקיפה לפונקציה"

#: awkgram.y:2065
#, c-format
msgid "attempt to use non-function `%s' in function call"
msgstr "נסיון שימוש בדבר שהוא לא בפונקציה `%s' בקרואת פןנקציה"

#: awkgram.y:2130
msgid "invalid subscript expression"
msgstr "ביטוי בעל תחביר שגוי במציין מערך"

#: awkgram.y:2505 awkgram.y:2525 gawkapi.c:274 gawkapi.c:291 msg.c:133
msgid "warning: "
msgstr "אזהרה: "

#: awkgram.y:2523 gawkapi.c:246 gawkapi.c:289 msg.c:165
msgid "fatal: "
msgstr "מכריע: "

#: awkgram.y:2576
msgid "unexpected newline or end of string"
msgstr "סוף שורה במקום בלתי-צפוי"

#: awkgram.y:2597
msgid ""
"source files / command-line arguments must contain complete functions or rules"
msgstr ""
"קובצי מקור / ארגומנטים של שורת הפקודה חייבים להכיל פונקציות מלאות או כללים"

#: awkgram.y:2881 awkgram.y:2959 awkgram.y:3197 debug.c:545 debug.c:561
#: debug.c:2844 debug.c:5213
#, c-format
msgid "cannot open source file `%s' for reading: %s"
msgstr "לא ניתן לפתוח את קובץ המקור `%s' לקריאה: %s"

#: awkgram.y:2882 awkgram.y:3019
#, c-format
msgid "cannot open shared library `%s' for reading: %s"
msgstr "לא ניתן לפתוח את התיקייה המשותפת `%s' לקריאה: %s"

#: awkgram.y:2884 awkgram.y:2960 awkgram.y:3020 builtin.c:136 debug.c:5364
msgid "reason unknown"
msgstr "מסיבה בלתי-ידועה"

#: awkgram.y:2893 awkgram.y:2917
#, c-format
msgid "cannot include `%s' and use it as a program file"
msgstr "לא יכול לכלול `%s' ולהשתמש בו כקובץ תוכנית"

#: awkgram.y:2906
#, c-format
msgid "already included source file `%s'"
msgstr "קובץ מקור `%s' כבר נכלל"

#: awkgram.y:2907
#, c-format
msgid "already loaded shared library `%s'"
msgstr "קובץ ספריה משותפת `%s' כבר נכלל"

#: awkgram.y:2944
msgid "@include is a gawk extension"
msgstr "‎@include היא הרחבה נקודתית ל־gawk"

#: awkgram.y:2950
msgid "empty filename after @include"
msgstr "שם קובץ ריק אחרי @include"

#: awkgram.y:2999
msgid "@load is a gawk extension"
msgstr "@load הינו הרחבה ספציפית ל-gawk"

#: awkgram.y:3006
msgid "empty filename after @load"
msgstr "שם קובץ ריק אחרי @load"

#: awkgram.y:3149
msgid "empty program text on command line"
msgstr "תכנית ריקה התקבלה משורת הפקודה"

#: awkgram.y:3265 debug.c:470 debug.c:628
#, c-format
msgid "cannot read source file `%s': %s"
msgstr "לא ניתן לקרוא את קובץ המקור `%s'‏: %s"

#: awkgram.y:3276
#, c-format
msgid "source file `%s' is empty"
msgstr "קובץ המקור `%s' ריק"

#: awkgram.y:3336
#, c-format
msgid "error: invalid character '\\%03o' in source code"
msgstr "שגיאה: התו `‎\\%03o' בקוד המקור שגוי"

#: awkgram.y:3563
msgid "source file does not end in newline"
msgstr "קובץ המקור אינו מסתיים בשורה חדשה"

#: awkgram.y:3673
msgid "unterminated regexp ends with `\\' at end of file"
msgstr "ביטוי רגולרי בלתי-גמור מסתיים ב-`\\' בסף הקובץ"

#: awkgram.y:3700
#, c-format
msgid "%s: %d: tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "%s: %d: משנה ביטוי רגולרי `/.../%c' של tawk לא עובד ב-gawk"

#: awkgram.y:3704
#, c-format
msgid "tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "משנה ביטוי רגולרי `/.../%c' של tawk לא עובד ב-gawk"

#: awkgram.y:3717
msgid "unterminated regexp"
msgstr "ביטוי רגולרי בלתי-גמור"

#: awkgram.y:3721
msgid "unterminated regexp at end of file"
msgstr "ביטוי רגולרי בלתי-גמור בסוף הקובץ"

#: awkgram.y:3810
msgid "use of `\\ #...' line continuation is not portable"
msgstr "שימוש ב-`\\ #...' כסימן להמשך שורה אינו פורטבילי"

#: awkgram.y:3832
msgid "backslash not last character on line"
msgstr "לוכסן הפוך שאינו תו אחרון בשורה"

#: awkgram.y:3880 awkgram.y:3882
msgid "multidimensional arrays are a gawk extension"
msgstr "מערכים רב-ממדיים הם הרחבה של gawk"

#: awkgram.y:3907 awkgram.y:3918
#, c-format
msgid "POSIX does not allow operator `%s'"
msgstr "POSIX אינו מרשה אופרטור `%s'"

#: awkgram.y:3909 awkgram.y:3920 awkgram.y:3955 awkgram.y:3963
#, c-format
msgid "operator `%s' is not supported in old awk"
msgstr "אופרטור `%s' אינו נתמך ב-awk ישן"

#: awkgram.y:4060 awkgram.y:4082 command.y:1188
msgid "unterminated string"
msgstr "מחרוזת בלתי-גמורה"

#: awkgram.y:4070 main.c:1284
msgid "POSIX does not allow physical newlines in string values"
msgstr "POSIX אינו מאפשר שורות חדשות פיזיות בערכי מחרוזת"

#: awkgram.y:4072 node.c:460
msgid "backslash string continuation is not portable"
msgstr "המשך מחרוזת נטוי אחורי אינו פורטיבילי"

#: awkgram.y:4313
#, c-format
msgid "invalid char '%c' in expression"
msgstr "תו שגוי '%c' בביטוי"

#: awkgram.y:4408
#, c-format
msgid "`%s' is a gawk extension"
msgstr "`%s' היא הרחבה ל־gawk"

#: awkgram.y:4413
#, c-format
msgid "POSIX does not allow `%s'"
msgstr "POSIX אינו מרשה `%s'"

#: awkgram.y:4421
#, c-format
msgid "`%s' is not supported in old awk"
msgstr "`%s' אינו נתמך ב-awk ישן"

#: awkgram.y:4521
msgid "`goto' considered harmful!"
msgstr "`goto' נחשב הרסני!"

#: awkgram.y:4590
#, c-format
msgid "%d is invalid as number of arguments for %s"
msgstr "%d הוא מספר שגוי של ארגומנטים עבור %s"

#: awkgram.y:4625
#, c-format
msgid "%s: string literal as last argument of substitute has no effect"
msgstr "%s: למחרוזת מילולית כארגומנט אחרון של תחליף אין השפעה"

#: awkgram.y:4630
#, c-format
msgid "%s third parameter is not a changeable object"
msgstr "ארגומנט %s שלישי של `sub' אינו פריט שניתן לשנות"

#: awkgram.y:4734 awkgram.y:4737
msgid "match: third argument is a gawk extension"
msgstr "ארגומנט שלישי של `match' הינו הרחבה ספציפית ל-gawk"

#: awkgram.y:4791 awkgram.y:4794
msgid "close: second argument is a gawk extension"
msgstr "ארגומנט שני של `close' הינו הרחבה ספציפית ל-gawk"

#: awkgram.y:4806
msgid "use of dcgettext(_\"...\") is incorrect: remove leading underscore"
msgstr "שימוש שגוי ב-dcgettext(\"...\"): יש לסלק מקף-תחתון שבקדמת ארגומנט"

#: awkgram.y:4821
msgid "use of dcngettext(_\"...\") is incorrect: remove leading underscore"
msgstr "שימוש שגוי ב-dcngettext(\"...\"): יש לסלק מקף-תחתון שבקדמת ארגומנט"

#: awkgram.y:4840
msgid "index: regexp constant as second argument is not allowed"
msgstr "index: קבוע ביטוי רגולרי כארגומנט שני אינו מותר"

#: awkgram.y:4893
#, c-format
msgid "function `%s': parameter `%s' shadows global variable"
msgstr "הפונקציה `%s': הפרמטר `%s' גובר על משתנה כללי"

#: awkgram.y:4942 debug.c:4197 debug.c:4240 debug.c:5362 profile.c:110
#, c-format
msgid "could not open `%s' for writing: %s"
msgstr "בעת פתיחת `%s' לצורך כתיבה תקלה (%s)"

#: awkgram.y:4943
msgid "sending variable list to standard error"
msgstr "רשימת משתנים נשלח לערוץ שגיאות תקני"

#: awkgram.y:4951
#, c-format
msgid "%s: close failed: %s"
msgstr "%s: תקלה בעת סגירת: %s"

#: awkgram.y:4976
msgid "shadow_funcs() called twice!"
msgstr "shadow_funcs()‎ נקראה פעמיים!"

#: awkgram.y:4984
msgid "there were shadowed variables"
msgstr "היו משתנים מאפילים"

#: awkgram.y:5061
#, c-format
msgid "function name `%s' previously defined"
msgstr "כבר מוגדרת פונקציה בשם `%s'"

#: awkgram.y:5112
#, c-format
msgid "function `%s': cannot use function name as parameter name"
msgstr "בפונקציה `%s': שם פונקציה אינו יכול לשמש כשם פרמטר"

#: awkgram.y:5115
#, c-format
msgid "function `%s': cannot use special variable `%s' as a function parameter"
msgstr "בפונקציה `%s': `%s' שם משתנה מיוחד אינו יכול לשמש כשם פרמטר"

#: awkgram.y:5119
#, c-format
msgid "function `%s': parameter `%s' cannot contain a namespace"
msgstr "הפונקציה `%s': הפרמטר `%s' לא יכול להכיל מרחב שם"

#: awkgram.y:5126
#, c-format
msgid "function `%s': parameter #%d, `%s', duplicates parameter #%d"
msgstr "הפונקציה `%s': פרמטר מס׳ %d,‏ `%s', משכפל את פרמטר מס׳ %d"

#: awkgram.y:5215
#, c-format
msgid "function `%s' called but never defined"
msgstr "קריאה לפונקציה `%s' שאינה מוגדרת"

#: awkgram.y:5219
#, c-format
msgid "function `%s' defined but never called directly"
msgstr "פונקציה `%s' הוגדרה, אך לא נקראת"

#: awkgram.y:5251
#, c-format
msgid "regexp constant for parameter #%d yields boolean value"
msgstr "ביטוי רגולרי קבוע למשתנה מס׳ %d מניב ערך בוליאני"

#: awkgram.y:5266
#, c-format
msgid ""
"function `%s' called with space between name and `(',\n"
"or used as a variable or an array"
msgstr ""
"קריאה לפןנקציה `%s' עם רווחים בין שמה לבין `(',\n"
"או משמש כמשתנה או מערך"

#: awkgram.y:5489 awkgram.y:5542 mpfr.c:1589 mpfr.c:1624
msgid "division by zero attempted"
msgstr "נסיון חלוקה באפס"

#: awkgram.y:5498 awkgram.y:5551 mpfr.c:1634
#, c-format
msgid "division by zero attempted in `%%'"
msgstr "נסיון חלוקה באפס ב-`%%'"

#: awkgram.y:5871
msgid "cannot assign a value to the result of a field post-increment expression"
msgstr "לא ניתן להקצות ערך לתוצאה של ביטוי שלאחר הגדלה של שדה"

#: awkgram.y:5874
#, c-format
msgid "invalid target of assignment (opcode %s)"
msgstr "יעד לא חוקי של הקצאה (אופקוד %s)"

#: awkgram.y:6254
msgid "statement has no effect"
msgstr "להכרזה אין השפעה"

#: awkgram.y:6769
