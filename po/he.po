msgstr "ערך מספר שלם שאינו אפס"

#: command.y:820
msgid ""
"backtrace [N] - print trace of all or N innermost (outermost if N < 0) frames"
msgstr ""
"backtrace [N] - הדפס עקבות של כל המסגרות או N הפנימיות ביותר (החיצוניות אם N < 0)"

#: command.y:822
msgid "break [[filename:]N|function] - set breakpoint at the specified location"
msgstr "break [[filename:]N|פונקציה] - הגדר נקודת עצירה במיקום שצוין"

#: command.y:824
msgid "clear [[filename:]N|function] - delete breakpoints previously set"
msgstr "clear [[filename:]N|פונקציה] - מחק נקודות שבירה שהוגדרו בעבר"

#: command.y:826
msgid ""
"commands [num] - starts a list of commands to be executed at a "
"breakpoint(watchpoint) hit"
msgstr ""
"commands [num] - מתחיל רשימה של פקודות לביצוע בפגע נקודת עצירה (נקודת שמירה)"

#: command.y:828
msgid "condition num [expr] - set or clear breakpoint or watchpoint condition"
msgstr "condition num [expr] - הגדר או נקה נקודת עצירה או מצב נקודת שמירה"

#: command.y:830
msgid "continue [COUNT] - continue program being debugged"
msgstr "continue [כמות] - המשך איתור באגים בתוכנית"

#: command.y:832
msgid "delete [breakpoints] [range] - delete specified breakpoints"
msgstr "delete [נקודות עצירה] [טווח] - מחיקת נקודות העצירה שצוינו"

#: command.y:834
msgid "disable [breakpoints] [range] - disable specified breakpoints"
msgstr "disable [נקודות עצירה] [טווח] - השבתת נקודות העצירה שצוינו"

# ------------------------------------------
#: command.y:836
msgid "display [var] - print value of variable each time the program stops"
msgstr "display [משתנה] - הדפסת ערך של משתנה בכל פעם שהתוכנית נעצרת"

#: command.y:838
msgid "down [N] - move N frames down the stack"
msgstr "down [ש] - ירידה של ש מסגרות במורד הערימה"

#: command.y:840
msgid "dump [filename] - dump instructions to file or stdout"
msgstr "dump [שם קובץ] - הטלת ההנחיות לקובץ או ל־stdout"

#: command.y:842
msgid "enable [once|del] [breakpoints] [range] - enable specified breakpoints"
msgstr "enable [once|del] [נקודות עצירה] [טווח] - הפעלת נקודות עצירה מסוימות"

#: command.y:844
msgid "end - end a list of commands or awk statements"
msgstr "end - סוף רשימת פקודות או הצהרות של awk"

#: command.y:846
msgid "eval stmt|[p1, p2, ...] - evaluate awk statement(s)"
msgstr "eval stmt|[p1, p2, ...] - חישוב ביטויי awk"

#: command.y:848
msgid "exit - (same as quit) exit debugger"
msgstr "exit - (כמו quit) יציאה ממנפה השגיאות"

#: command.y:850
msgid "finish - execute until selected stack frame returns"
msgstr "finish - להריץ עד שמסגרת המחסנית הנבחרת חוזרת"

#: command.y:852
msgid "frame [N] - select and print stack frame number N"
msgstr "frame [ש] - בחירה והדפסה של מסגרת מס׳ ש"

#: command.y:854
msgid "help [command] - print list of commands or explanation of command"
msgstr "help [פקודה] - הדפסת רשימת פקודות או הסבר של פקודה"

#: command.y:856
msgid "ignore N COUNT - set ignore-count of breakpoint number N to COUNT"
msgstr "ignore N COUNT - הגדרת כמות התעלמות של נקודת העצירה N לכדי COUNT"

#: command.y:858
msgid ""
"info topic - source|sources|variables|functions|break|frame|args|locals|display|"
"watch"
msgstr ""
"info topic - "
"source|sources|variables|functions|break|frame|args|locals|display|watch"

#: command.y:860
msgid "list [-|+|[filename:]lineno|function|range] - list specified line(s)"
msgstr "list [-|+|[filename:]lineno|function|range] - הצגת השורות שצוינו"

#: command.y:862
msgid "next [COUNT] - step program, proceeding through subroutine calls"
msgstr "next [כמות] - צעידה בתוכנית, תוך התקדמות מבעד לקריאת לתת־שגרות"

#: command.y:864
msgid "nexti [COUNT] - step one instruction, but proceed through subroutine calls"
msgstr "nexti [כמות] - התקדמות בהנחיה אחת, אבל להמשיך מבעד לקריאות לתת־השגרות"

#: command.y:866
msgid "option [name[=value]] - set or display debugger option(s)"
msgstr "option [name[=value]] - הגדרת או הצגת אפשרויות מנפה שגיאות"

#: command.y:868
msgid "print var [var] - print value of a variable or array"
msgstr "print var [משתנה] - הצגת ערך של משתנה או מערך"

#: command.y:870
msgid "printf format, [arg], ... - formatted output"
msgstr "printf format, [arg], ...‎ - פלט מעוצב"

#: command.y:872
msgid "quit - exit debugger"
msgstr "quit - יציאה ממנפה השגיאות"

#: command.y:874
msgid "return [value] - make selected stack frame return to its caller"
msgstr "return [ערך] - לגרום למסגרת הערימה הנבחרת לחזור למי שקרא לה"

#: command.y:876
msgid "run - start or restart executing program"
msgstr "run - התחלת או חידוש הרצת תוכנית"

#: command.y:879
msgid "save filename - save commands from the session to file"
msgstr "save filename - שמירת פקודות מההפעלה לקובץ"

#: command.y:882
msgid "set var = value - assign value to a scalar variable"
msgstr "set var = value - החלת ערך למשתנה סקלארי"

#: command.y:884
msgid "silent - suspends usual message when stopped at a breakpoint/watchpoint"
msgstr "silent - עיכוב ההודעות הרגילות בזמן עצירה על נקודת עצירה/מעקב"

#: command.y:886
msgid "source file - execute commands from file"
msgstr "קובץ מקור -- הרצת פקודות מקובץ"

#: command.y:888
msgid "step [COUNT] - step program until it reaches a different source line"
msgstr "step [COUNT] - לעבור צעד צעד בתוכנית עד להגעה לשורת מקור אחרת"

#: command.y:890
msgid "stepi [COUNT] - step one instruction exactly"
msgstr "stepi [כמות] - להתקדם הנחיה אחת בלבד"

#: command.y:892
msgid "tbreak [[filename:]N|function] - set a temporary breakpoint"
msgstr "tbreak [[filename:]N|function] - הגדרת נקודת עצירה זמנית"

#: command.y:894
msgid "trace on|off - print instruction before executing"
msgstr "trace on|off - להדפיס את ההנחיה לפני הרצתה"

#: command.y:896
msgid "undisplay [N] - remove variable(s) from automatic display list"
msgstr "undisplay [N] - הסרת משתנים מרשימת תצוגה אוטומטית"

#: command.y:898
msgid ""
"until [[filename:]N|function] - execute until program reaches a different line "
"or line N within current frame"
msgstr ""
"until [[filename:]N|function] - להריץ עד שהתוכנית מגיעה לשורת אחרת או לשורה "
"N במסגרת הנוכחית"

#: command.y:900
msgid "unwatch [N] - remove variable(s) from watch list"
msgstr "unwatch [N] - הסרת משתנים מרשימת המעקב"

#: command.y:902
msgid "up [N] - move N frames up the stack"
msgstr "up [N] - העלאת N מסגרות במעלה הרשימה"

#: command.y:904
msgid "watch var - set a watchpoint for a variable"
msgstr "watch var - הגדרת נקודת מעקב למשתנה"

#: command.y:906
msgid ""
"where [N] - (same as backtrace) print trace of all or N innermost (outermost if "
"N < 0) frames"
msgstr ""
"where [N] - (זהה ל-backtrace) הדפס עקבות של כל המסגרות או N הפנימיות ביותר (החיצוניות אם N < 0)"

